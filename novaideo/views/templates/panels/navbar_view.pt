<html xmlns:tal="http://xml.zope.org/namespaces/tal"
  tal:omit-tag="">
   <nav id="navbaruser" class="navbar navbar-default navbar" role="navigation">
     <div class="container">
        <div class="row global-actions-menu" tal:define="keys list(actions['menu3'].keys())">
            <div class="col-md-3 action-1"  tal:condition="actions['menu3']" tal:define="k keys[0]; a actions['menu3'][k]">
                <a class="btn btn-default ${((a is None) and 'disabled') or ''} btn-sm"  href="${a}" >
                   ${k}
                </a>
            </div>
            <div class="col-md-3"  tal:condition="actions['menu3']" tal:define="k keys[1]; a actions['menu3'][k]">
                <a class="btn btn-default ${((a is None) and 'disabled') or ''} btn-sm"  href="${a}" >
                   ${k}
                </a>
            </div>
       </div>
      <!--a class="btn btn-navbar btn-primary nav-control" data-toggle="collapse" data-target=".nav-collapse"-->
          <!--span class="glyphicon glyphicon-chevron-up"></span-->
      <!--/a-->
        <div class="row">
            <div class="col-sm-6 col-md-4" style="border-right: 1px inset windowtext;">
                ${structure:search_body}
            </div>
            <div class="col-sm-6 col-md-6">
                    <div class="row">
                        <div class="col-md-6">
                          <ul tal:condition="actions['menu1']"  class="nav nav-pills nav-stacked">
                             <li tal:repeat="(k, a) actions['menu1'].items()">
                                <a class="btn btn-primary ${((actions['menu1'][k] is None) and 'disabled') or ''} btn-sm" style="max-width: 300px;" href="${actions['menu1'][k]}" >
                                   ${k}
                                </a>
                             </li>
                          </ul>
                        </div>
                        <div class="col-md-6" >
                          <ul tal:condition="actions['menu2']"  class="nav nav-pills nav-stacked">
                             <li tal:repeat="(k, a) actions['menu2'].items()">
                                <a class="btn btn-primary ${((actions['menu2'][k] is None) and 'disabled') or ''} btn-sm" style="max-width: 300px;" href="${actions['menu2'][k]}" >
                                   ${k}
                                </a>
                             </li>
                          </ul>
                      </div>
                 </div>
            </div>
         </div>
      </div>
   </nav>
</html>
