<div class="compare-result" i18n:domain="novaideo">
 <div tal:omit-tag="" tal:condition="version">
  <div class="object-well proposal-well">
    <dl >
      <dt  i18n:translate="">Abstract</dt>
      <dd>${structure:descriptiondiff}</dd>
      <dt tal:condition="keywordsdiff"  i18n:translate="">Key words</dt>
      <dd tal:condition="keywordsdiff">
        <tal:loop repeat="k keywordsdiff">
          <a tal:attributes="href request.resource_url(request.root, '', query={'text':k['title']})"><del tal:condition="k['state']=='del'">${k['title']}</del><ins tal:condition="k['state']=='ins'">${k['title']}</ins><span tal:condition="k['state']=='nothing'">${k['title']}</span></a>
            <tal:separator condition="not: repeat['k'].end">, </tal:separator> 
        </tal:loop>
      </dd>
    </dl>
  </div>
  <div class="content-text" id="contenttext">
    ${structure:textdiff}
  </div>
  <script>
    init_content_text()
  </script>
  <div id="legend">
    <div i18n:translate=""><div class="legend-carre-ins"></div> : Texte ajouté dans ${idee.title}</div>
    <div i18n:translate=""><div class="legend-carre-del"></div> : Texte supprimé dans ${idee.title}</div>
  </div>
 </div>
</div>
