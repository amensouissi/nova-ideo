<div class="thumbnail">
        <img tal:condition="logo" tal:attributes="src logo['url']" alt="300x200" />
</div>
<dl class="dl-horizontal">
    <dt>${title}</dt>
    <dd>
        <p class="text-muted" tal:condition="description">
            <small>${description}</small>
        </p>
    </dd>
    <dt tal:condition="email">Email</dt><dd>${email}</dd>
    <dt tal:condition="phone">Phone</dt><dd>${phone}</dd>
    <dt tal:condition="fax">Fax</dt><dd>${fax}</dd>
    <dt>Members</dt>
    <dd>
      <ul class="nav nav-list">
        <li tal:repeat="m members">${m}</li>
      </ul>
    </dd>
</dl>
